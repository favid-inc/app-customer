{
  "name": "@favid-inc/app-customer",
  "version": "2.0.1",
  "private": true,
  "author": "akveo",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear",
    "start:dev": "npm run env:set -- dev && npm start",
    "start:prod": "npm run env:set -- prod && npm start",
    "env:set": "./scripts/environment/set-env.sh",
    "build": "tsc -p ./tsconfig.json",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "clean": "rimraf ./dist",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint",
    "ci:build": "npm run env:set -- prod && npm run build",
    "conventional-changelog": "conventional-changelog",
    "version:changelog": "npm run conventional-changelog -- -i ./CHANGELOG.md -s"
  },
  "dependencies": {
    "@eva-design/eva": "^1.3.0",
    "@favid-inc/api": "^5.2.3",
    "axios": "^0.19.0",
    "expo": "^35.0.0",
    "expo-analytics": "^1.0.13",
    "expo-app-auth": "~7.0.0",
    "expo-av": "~7.0.1",
    "expo-constants": "~7.0.0",
    "expo-facebook": "~7.0.0",
    "firebase": "^6.6.2",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-modal": "^11.5.3",
    "react-native-ui-kitten": "^4.3.1",
    "react-native-video": "^4.4.4",
    "react-navigation": "^3.13.0",
    "scheduler": "^0.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@types/react": "^16.9.14",
    "@types/react-native": "^0.57.65",
    "@types/react-navigation": "^3.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "husky": "^1.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
